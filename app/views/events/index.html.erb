<h1>Welcome to Events Website</h1>


<div class="container">
	<% @events.each do |event| %>
	  <div class="well">
		<p><%= event.title %></p>
	    <p><%= event.description %></p>
	    <p><%= event.event_date %></p>
	    <p><%= event.venue %></p>
	    <p><%= event.address %></p>
	    <p><%= event.city %></p>
	    <p><%= event.state %></p>
	  </div>
	<% end %>
</div>